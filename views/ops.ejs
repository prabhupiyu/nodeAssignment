}
</style>
</head>
  <body>
    
        
    <div class="container" style="top:10%; position:absolute;">
       <!-- <h2>Basic Panel</h2>-->
        <div class="panel panel-default" >
      
        
            <br><br>
        <form action="/api/search" method="post" >
            <input type="text" placeholder="Serach for.." name="dispnm">
            <button type="submit" >Go!</button>
        </form>
        
           <form action="/api/logout" method="post" style=" float:right; margin-right:20px">
            <button type="submit" style="float:right">LOgout</button>
        </form>
            
            
            <table style="width:50%; margin:0% 20%">
            <tr>
                <th>First Name</th>
                <th>Last Name</th>
                 <th>Display Name</th>
                <th>Status</th>
                </tr>
                
            <% for(var i=0; i< myobject2.users.length; i++) {%>
                <tr>
                        <td>
                    <%= myobject2.users[i].attributes['First Name'] %>
                        </td>
          
                
                 
                        <td>
                    <%= myobject2.users[i].attributes['Last Name'] %>
                        </td>
            
                
                    <td>
                    <%= myobject2.users[i].attributes['Display Name'] %>
                        </td>
                                              
                        <td>
                    <%= myobject2.users[i].attributes['Status'] %>
                        </td>
            </tr>
        <% } %>
            </table>
        
         </div>
        </div>
  
     
                    
    </div>
            
